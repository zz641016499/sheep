/* 箭头 */
.sign {
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%) rotate(-90deg);
  transition: all 0.5s ease;
  display: block;
  width: 34rpx;
  height: 34rpx;
  float: right;
  background-repeat: no-repeat;
  background-size: 16rpx 24rpx;
  background-position: center center;
  background-image: url('data:image/png;base64,');
}


